local rekt = Instance.new('ColorCorrectionEffect', game.Lighting)
		rekt.TintColor = Color3.new(155, 1, 0)
		rekt.Brightness = 0.2
		rekt.Contrast = 1
		rekt.Saturation = 1
		local topkek = Instance.new('BlurEffect', game.Lighting)
		topkek.Size = 3
		local bloom = Instance.new('BloomEffect', game.Lighting)
		bloom.Intensity = 0.4
		bloom.Size = 56
		bloom.Threshold = 1
